{
  "gpu_vendor": "AMD",
  "guest_os": "UBUNTU",
  "tools": [
    {
      "name": "rocprofv3_full",
      "env_vars": {
        "RCCL_DEBUG": "INFO",
        "HSA_ENABLE_SDMA": "0"
      }
    },
    {
      "name": "gpu_info_power_profiler",
      "env_vars": {
        "POWER_DEVICE": "all",
