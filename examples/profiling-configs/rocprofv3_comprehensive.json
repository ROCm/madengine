    {
      "name": "gpu_info_vram_profiler",
      "env_vars": {
        "VRAM_DEVICE": "all",
        "VRAM_SAMPLING_RATE": "0.1"
      }
    },
    {
      "name": "miopen_trace"
    },
    {
      "name": "rocblas_trace"
    }
  ]
}
